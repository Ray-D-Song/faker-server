{
  "name": "faker-server",
  "version": "0.1.0",
  "description": "Faker.js mock server",
  "scripts": {
    "dev:client": "pnpm --filter client dev",
    "build:client": "pnpm --filter client build",
    "dev:server": "pnpm --filter server dev",
    "build:server:cf": "pnpm --filter server build:cf",
    "build:server:tc": "pnpm --filter server build:tc",
    "build": "pnpm run build:server:cf && pnpm run build:client"
  },
  "keywords": [
    "faker",
    "mock",
    "server",
    "mock-server",
    "faker-server",
    "faker.js",
    "fakerjs"
  ],
